{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": ".",
  "framework": null,
  "rewrites": [
    {
      "source": "/",
      "destination": "/index.html"
    },
    {
      "source": "/login",
      "destination": "/login.html"
    },
    {
      "source": "/room",
      "destination": "/room.html"
    },
    {
      "source": "/stats",
      "destination": "/stats.html"
    }
  ]
}

